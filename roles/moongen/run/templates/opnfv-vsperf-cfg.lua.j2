VSPERF {
startRate = {{ startRate }},
nrFlows = 256,
vlanIds = { {{ vlan_id }},{{ vlan_max_range }} },
testType = "throughput",
runBidirec = {{ runBidirec }},
searchRunTime = {{ searchRunTime }},
validationRunTime = {{ validationRunTime }},
acceptableLossPct = {{ acceptableLossPct }},
frameSize = 64,
{% if srcMac is not defined or srcMac == "" %}
--srcMac = ,
{% else %}
srcMac = {{ srcMac }},
{% endif %}
{% if dstMac is not defined or dstMac == "" %}
--dstMacs = ,
{% else %}
dstMacs = {{ dstMac }},
{% endif %}
ports = {{ ports }},
}
